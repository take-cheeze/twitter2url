{
    "name": "twi2url",
    "version": "0.0.3",
    "description": "Collect url from your twitter.",

    "background_page": "background.html",
    "browser_action": {
        "default_icon": "icons/bird_32_blue.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/bird_16_blue.png",
        "32": "icons/bird_32_blue.png",
        "48": "icons/bird_48_blue.png"
    },
    "permissions": [
        "background",
        "history",
        "tabs",
        "unlimitedStorage",
        "https://api.twitter.com/*",
        "http://api.longurl.org/v2/*",
        "http://api.twitpic.com/2/*",
        "http://yfrog.com/*",
        "http://www.pixiv.net/*",
        "http://instagr.am/p/*",
        "http://p.twipple.jp/*",
        "http://seiga.nicovideo.jp/seiga/*",
        "http://soundtracking.com/tracks/*"
    ],
    "options_page": "options.html"
}
