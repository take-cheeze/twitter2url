{
    "name": "twi2url",
    "version": "0.0.6",
    "description": "Collect url from your twitter.",

    "background_page": "page/background.html",
    "browser_action": {
        "default_icon": "icons/bird_32_blue.png",
        "default_popup": "page/popup.html"
    },
    "icons": {
        "16": "icons/bird_16_blue.png",
        "32": "icons/bird_32_blue.png",
        "48": "icons/bird_48_blue.png"
    },
    "permissions": [
        "background", "history", "tabs", "unlimitedStorage",
        "http://*/*", "https://*/*"
    ],
    "options_page": "page/options.html"
}
