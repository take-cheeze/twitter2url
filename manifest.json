{
    "name": "twi2url",
    "version": "0.0.6",
    "description": "Collect url from your twitter.",

    "background_page": "background.html",
    "browser_action": {
        "default_icon": "icons/bird_32_blue.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/bird_16_blue.png",
        "32": "icons/bird_32_blue.png",
        "48": "icons/bird_48_blue.png"
    },
    "permissions": [
        "background",
        "history",
        "tabs",
        "unlimitedStorage",
        "https://api.twitter.com/*",
        "http://api.longurl.org/v2/*",
        "http://api.twitpic.com/2/*",
        "http://yfrog.com/*",
        "http://www.pixiv.net/*",
        "http://instagr.am/p/*",
        "http://p.twipple.jp/*",
        "http://seiga.nicovideo.jp/seiga/*",
        "http://soundtracking.com/tracks/*",
        "http://img.ly/*",
        "http://lockerz.com/s/*",
        "http://api.photozou.jp/rest/photo_info*",
        "http://pikubo.jp/photo/*",
        "http://picplz.com/user/*",
        "http://www.mobypicture.com/user/*",
        "http://english.aljazeera.net/*",
        "http://www.youtube.com/oembed*",
        "http://vimeo.com/api/oembed.json*",
        "http://www.slideshare.net/api/oembed/2*",
        "http://flickr.com/services/oembed*",
        "http://soundcloud.com/oembed*"
    ],
    "options_page": "options.html"
}
